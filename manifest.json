{
    "manifest_version": 2,

    "name": "Quizlet Hack Extension",
    "description": "A Chrome Extension to Pause the Timer on the Quizlet Matching Game",
    "version": "1.0",

    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab"
    ],

    "content_scripts": [{
        "matches": [ "<all_urls>" ],
        "js": [ "popup.js" ]
    }]
}